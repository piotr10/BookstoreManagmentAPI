{
  "ConnectionStrings": {
    "BookstoreDatabase4": "Server=localhost\\SQLEXPRESS;Database=BookstoreDatabase4;Trusted_Connection=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },

  "AllowedHosts": "*"
}
